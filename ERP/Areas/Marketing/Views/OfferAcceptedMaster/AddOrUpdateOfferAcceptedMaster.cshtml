<script type="text/javascript">
    var urlsave = '@Url.Action("AddOrUpdateOfferAcceptedMaster", "OfferAcceptedMaster")';
    function fnSubmit() {

        var sapStringJson = document.getElementById('SAPCollectionJSONString');

        var finalTable = document.getElementsByTagName('table');
        var rowLen = finalTable[0].rows.length;

        var tableData = [];
        for (var i = 1; i < rowLen; i++) {

            let OfferAcceptedDetailId = finalTable[0].rows[i].cells[1].children[0].value;
            if (OfferAcceptedDetailId === '') {
                OfferAcceptedDetailId = 0;
            }
            let OfferAcceptedId = finalTable[0].rows[i].cells[2].children[0].value;
            if (OfferAcceptedId === '') {
                OfferAcceptedId = 0;
            }
            let itemId = finalTable[0].rows[i].cells[3].children[0].value;
            if (itemId === '') {
                itemId = 0;
            }
            var rowData = {

                OfferAcceptedDetailID: OfferAcceptedDetailId,
                OfferAcceptedID: OfferAcceptedId,
                ItemID: itemId,
                UOMID: finalTable[0].rows[i].cells[4].children[0].value,
                ItemCode: finalTable[0].rows[i].cells[5].children[0].value,
                ItemName: finalTable[0].rows[i].cells[6].children[0].value,
                HSNcodes: finalTable[0].rows[i].cells[8].children[0].value,
                // UOM: finalTable[0].rows[i].cells[0].children[0].value,
                Qty: finalTable[0].rows[i].cells[10].children[0].value,
                Rate: finalTable[0].rows[i].cells[11].children[0].value,
                TotalAmount: finalTable[0].rows[i].cells[12].children[0].value

            };

            tableData.push(rowData);
        }
        sapStringJson.value = JSON.stringify(tableData);

        if ($("#formOfferAcceptedMaster").valid()) {
            var l = laddaStart('btnSave');
            console.log(l);
            modeldata = $("#formOfferAcceptedMaster").serialize();
            $.ajax({
                type: 'POST',
                url: urlsave,
                data: modeldata,
                dataType: 'json',
                beforeSend: function () { },
                success: function (data) {
                    if (data.status) {
                        //notification
                        // $("#canvas_masterentity2").hide();//('hide');
                        Lobibox.notify('success', {
                            pauseDelayOnHover: true,
                            size: 'mini',
                            icon: 'bx bx-check-circle',
                            continueDelayOnInactiveTab: false,
                            position: 'bottom right',
                            msg: data.message
                        });
                        window.location.href = '/Marketing/OfferAcceptedMaster/';
                    }
                    else {
                        Lobibox.notify('error', {
                            pauseDelayOnHover: true,
                            size: 'mini',
                            icon: 'bx bx-check-circle',
                            continueDelayOnInactiveTab: false,
                            position: 'bottom right',
                            msg: data.message
                        });
                    }
                },
                error: function (error) {
                    laddaStop(l);
                    Lobibox.notify('error', {
                        pauseDelayOnHover: true,
                        size: 'mini',
                        icon: 'bx bx-check-circle',
                        continueDelayOnInactiveTab: false,
                        position: 'bottom right',
                        msg: "@MessageHelper.Error"
                    });
                }
            }).always(function () {
                hideLoader();
            });
        }
    }

