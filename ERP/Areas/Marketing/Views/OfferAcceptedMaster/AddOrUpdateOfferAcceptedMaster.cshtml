    function removeRow(button) {

        var row = button.parentNode.parentNode.parentNode;

        let OfferAcceptedDetailId = row.cells[1].children[0].value;
        let OfferAcceptedId = row.cells[2].children[0].value;
        var params = { OfferAcceptedDetailId: OfferAcceptedDetailId, OfferAcceptedId: OfferAcceptedId };
        $.ajax({
            type: 'POST',
            url: '/Marketing/OfferAcceptedMaster/OfferAcceptedDetailItemDeactivation',
            data: params,
            dataType: 'json',
            success: function (data) {
                if (data.status) {
                    //notification
                    // $("#canvas_masterentity2").hide();//('hide');
                    Lobibox.notify('success', {
                        pauseDelayOnHover: true,
                        size: 'mini',
                        icon: 'bx bx-check-circle',
                        continueDelayOnInactiveTab: false,
                        position: 'bottom right',
                        msg: 'Item removed.'
                    });
                    //window.location.href = '/Marketing/OfferAcceptedMaster/';
                    row.parentNode.removeChild(row);
                }
                else {
                    Lobibox.notify('error', {
                        pauseDelayOnHover: true,
                        size: 'mini',
                        icon: 'bx bx-check-circle',
                        continueDelayOnInactiveTab: false,
                        position: 'bottom right',
                        msg: 'Something went wrong to remove item.'
                    });
                }
            },
            error: function (error) {
                laddaStop(l);
                Lobibox.notify('error', {
                    pauseDelayOnHover: true,
                    size: 'mini',
                    icon: 'bx bx-check-circle',
                    continueDelayOnInactiveTab: false,
                    position: 'bottom right',
                    msg: "@MessageHelper.Error"
                });
            }
        }).always(function () {
            hideLoader();
        });

    }

