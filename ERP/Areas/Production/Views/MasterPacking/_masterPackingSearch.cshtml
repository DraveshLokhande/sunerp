@model Business.Entities.Production.MasterPackingModel.MasterPackingSearch
@{

}
<form method="get" id="frmMasterPackingSearch" asp-action="Search" asp-controller="MasterPacking" asp-area="Production" class="g-3" novalidate>
    <div class="mt-3 row">
        <div class="col-md-6">
            <div class="mb-3 row">
                <div class="col-md-4">
                    <label for="Keyword" class="col-form-label">Search keyword</label>
                </div>
                <div class="col-md-8">
                    <input type="text" class="form-control" asp-for="keyword" maxlength="50" placeholder="----- Search By Input Value -------">
                </div>
            </div>
        </div>
        <div class="col-md-6" style="">
            <div class="row">
                <div class="col-6">
                    <div class="text-end">
                        <button class="btn btn-primary btn-info px-5 ladda-button" type="submit">
                            Search
                        </button>
                    </div>
                </div>
                <div class="col-6">
                    <div class="text-end">
                        <input type="button" class="btn btn-primary btn-info px-5 ladda-button" value="Print All QR Code" onclick="fnPrintAllQRCode()">
                    </div>
                </div>
            </div>
            
        </div>
    </div>
</form>
@await Html.PartialAsync("_ValidationScriptsPartial")
<script type="text/javascript">
    function fnPrintAllQRCode() {
        alert("fnPrintAllQRCode Success!");
    }

</script>